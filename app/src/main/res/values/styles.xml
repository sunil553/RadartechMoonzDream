<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>



    <!-- Custom dialog animation -->
    <style name="DialogTheme">
        <item name="android:windowEnterAnimation">@anim/frag_enter</item>
        <item name="android:windowExitAnimation">@anim/frag_exit</item>
    </style>

    <style name="magellan_button" parent="match_wrap">
        <item name="android:textSize">@dimen/login_screen_textsize</item>
        <item name="android:layout_marginLeft">@dimen/login_button_margin</item>
        <item name="android:layout_marginRight">@dimen/login_button_margin</item>
        <item name="android:layout_marginTop">@dimen/button_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/et_margin_bottom</item>
        <item name="android:background">@drawable/button_ripple_effect</item>
        <item name="android:textColor">@drawable/button_text_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:padding">@dimen/edittext_padding</item>
        <item name="typeface">ubuntu_medium</item>
    </style>


    <!--Base themes-->
    <style name="match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="wrap_match">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
</style>

    <style name="wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="match_45">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="image_size">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">100dp</item>
    </style>

    <!-- toggle button -->
    <style name="Toggel_button"     parent="wrap_wrap">
        <item name="colorControlActivated">#ffffff</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#a4c639</item>

    </style>






    <!--clear and new buttons-->

    <style name="create_new"
        >
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginTop">@dimen/margin4</item>
        <item name="android:textSize">@dimen/margin12</item>
        <item name="android:visibility">gone</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginRight">@dimen/margin10</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@color/white_title</item>
    </style>






    <!--textcolors  alarams-->

    <style name="textcolore" parent="wrap_wrap">
        <item name="android:textSize">@dimen/textsize</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">@dimen/margin20</item>
        <item name="android:layout_marginStart">@dimen/margin20</item>
        <item name="android:layout_centerVertical">true</item>
    </style>




    <!--Radio_button-->

    <style name="Radio_button" parent="wrap_wrap">
        <item name="android:textColor">@color/black</item>
    </style>


    <!--time and date selector -->

    <style name="time_date" parent="wrap_wrap">
        <item name="android:layout_gravity">start|center</item>
        <item name="android:textSize">20sp</item>
    </style>




    <!--textview_timeselector -->

    <style name="textview_timeselector" parent="wrap_wrap">
        <item name="android:layout_gravity">start|center</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginLeft">@dimen/margin20</item>
        <item name="android:layout_marginStart">@dimen/margin20</item>
        <item name="android:background">@drawable/input</item>
        <item name="android:paddingLeft">2dp</item>
    </style>


    <!--dialog_frnce_info_textview-->

    <style name="dialog_frnce_info_textview" parent="wrap_wrap">
        <item name="android:singleLine">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">@dimen/margin5</item>
        <item name="android:layout_marginStart">@dimen/margin5</item>

    </style>
    <!--itemalarm_list_text-->

    <style name="itemalarm_list_text" parent="wrap_wrap">
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:text">Medium Text</item>
    </style>


    <!--relativelayout-->


    <style name="relative_layout" parent="match_45">
    <item name="android:layout_marginLeft">@dimen/location_textsize_info_padding</item>
    <item name="android:layout_marginRight">@dimen/location_textsize_info_padding</item>
    </style>


    <!--righticon-->

    <style name="right_icon">
        <item name="android:layout_width">@dimen/margin25</item>
        <item name="android:layout_height">@dimen/margin25</item>
        <item name="android:layout_marginEnd">@dimen/margin20</item>
        <item name="android:layout_marginRight">@dimen/margin20</item>
        <item name= "android:layout_centerVertical">true</item>
        <item name= "android:layout_alignParentEnd">true</item>
        <item name= "android:layout_alignParentRight">true</item>
        <item name= "android:src">@drawable/icon_right</item>
        <item name= "android:contentDescription">@string/load_image_failed</item>

    </style>


   <!-- loginimages-->
    <style name="login_images">
        <item name="android:layout_width">@dimen/margin40</item>
        <item name="android:layout_height">@dimen/margin40</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name= "android:contentDescription">@string/load_image_failed</item>

    </style>


    <!-- logincheckbox-->
    <style name="login_checkbox" parent="wrap_wrap">

        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginLeft">15sp</item>


        <item name= "android:contentDescription">@string/load_image_failed</item>

    </style>


    <!--mapsimages-->
    <style name="map_images"  parent="wrap_wrap">

    </style>

    <!--geofence buttons-->

    <style name="button_size">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/margin40</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/button_blue</item>
    </style>


    <!--deviceTracker-->
    <style name="device_tracker">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/set_normal_send</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
    </style>


    <!--imageview_devicetracker-->
    <style name="image_view">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:contentDescription">@string/load_image_failed</item>
        <item name="android:orientation">vertical</item>
    </style>


    <!--text_devicetracker-->
    <style name="text_view"  parent="wrap_wrap" >

        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!--update device-->
    <style name="ubdate_device"
         >

        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>

    </style>
    <!--edit text  update device-->
    <style name="edit_text_update_device">

        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">@dimen/margin10</item>
        <item name="android:layout_marginStart">@dimen/margin10</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/margin18</item>

    </style>






    <!--linearlayout  update device-->
    <style name="linearlayout_update_device"   parent="match_wrap">
        <item name="android:layout_marginLeft">@dimen/margin5</item>
        <item name="android:layout_marginStart">@dimen/margin5</item>
        <item name="android:orientation">horizontal</item>


    </style>






    <!--dialoge_playback_testview -->
    <style name="dialoge_playback_testview"   parent="match_wrap">
        <item name="android:layout_marginLeft">@dimen/margin10</item>
        <item name="android:layout_marginStart">@dimen/margin10</item>
        <item name="android:textColor">@color/black</item>


    </style>



    <style name="modifily_passsword"   parent="wrap_wrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">@dimen/margin20</item>
        <item name="android:background">@drawable/input</item>
        <item name="android:paddingEnd">0dp</item>
        <item name="android:paddingLeft">@dimen/margin10</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingStart">@dimen/margin10</item>
        <item name="android:inputType">textPassword</item>

    </style>

    <!--infoactivity-->
    <style name="info_activity"   parent="wrap_wrap">

        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/margin15</item>
        <item name="android:layout_marginStart">@dimen/margin10</item>
        <item name="android:textColor">@color/white_title</item>
        <item name="android:textSize">@dimen/margin20</item>

    </style>

    <!--imageview_infoactivity-->

    <style name="imageview_infoactivity">

        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/margin15</item>
        <item name="android:layout_marginStart">@dimen/margin15</item>
        <item name="android:contentDescription">@string/load_image_failed</item>

    </style>

    <!--millagetextview-->
    <style name="millage_textview"     parent="wrap_wrap">

        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:singleLine">false</item>
        <item name="android:layout_marginStart">@dimen/margin5</item>
        <item name="android:textSize">16dp</item>

    </style>

    <!--speedbutton-->
    <style name="speed_button"     >

        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:padding">@dimen/margin10</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:contentDescription">@string/load_image_failed</item>

    </style>

    <!--mainxml-->
    <style name="main_xml"     parent="match_wrap">
        <item name="android:padding">@dimen/margin10</item>
        <item name="android:layout_marginTop">@dimen/margin8</item>

    </style>
    <!--main_text xml-->
    <style name="main_text"     parent="match_wrap">
        <item name="android:layout_marginTop">@dimen/margin8</item>

    </style>


    <!--radiobutton-->
    <style name="radio_button"     parent="wrap_wrap">
        <item name="android:textColor">@color/black</item>

    </style>

    <!--weeks textview-->

    <style name="days_text"     parent="wrap_wrap">
        <item name="android:textSize">@dimen/margin16</item>

    </style>


    <!--geofence create new activity-->

    <style name="activity_create">
        <item name="android:layout_width">@dimen/size</item>
        <item name="android:layout_height">@dimen/size</item>
        <item name="android:layout_marginEnd">@dimen/size1</item>
        <item name="android:layout_marginRight">@dimen/size1</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!--title in all layout-->
    <style name="style_title_text" parent="wrap_wrap">
        <item name="android:textSize">@dimen/title_textsize</item>
        <item name="android:textColor">@color/white_title</item>
        <item name="android:gravity">center</item>
    </style>

    <!--dialog_marker_info-->
    <style name="style_dialog_marker_info_text" parent="wrap_wrap">
        <item name="android:textSize">@dimen/dialog_marker_info_textsize</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!--google map bottom adres bar-->
    <style name="style_location_info_text" parent="match_wrap">
        <item name="android:textSize">@dimen/location_textsize_info_textsize</item>
        <item name="android:padding">@dimen/location_textsize_info_padding</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/new_line_grey</item>
    </style>

    <style name="push_style">
        <item name="android:hint">@string/style_hint</item>
        <item name="android:inputType">number</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="push_alias">
        <item name="android:hint">@string/alias_hint</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="push_tag">
        <item name="android:hint">@string/tag_hint</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="AppTheme.Fullscreen">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

</resources>